public class ProductoDao implements IProductoDao extend DataMapper {

	@Override
	public Hashtable<String, String> getMenu() {
		try {
			Connection cn = AccesoDB.getConnection();
			String sql="select * from producto";
			Statement stm = cn.createStatement();
			ResultSet rs = stm.executeQuery(sql);
			while(rs.next()){
				
			}
		} catch (Exception e) {
			// TODO: handle exception
		}
		return null;
	}

}
