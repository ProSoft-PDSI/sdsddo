package dao.mapper;

import java.sql.ResultSet;
import java.sql.SQLException;

import dao.util.DataMapper;
import entity.Producto;

public class ProductoMapper extends DataMapper<Producto> {

	public void mapData(ResultSet rs) {
		Producto bean = new Producto();
		try {
			bean.setCodProducto(rs.getString("codp"));
			bean.setNombre("descrproducto");
			bean.setPrecio(rs.getDouble("preciounitario"));
			bean.setStock(rs.getInt("stock"));
		} catch (SQLException e) {
			e.printStackTrace();
		}
		
	}

}
